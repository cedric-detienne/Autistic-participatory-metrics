  url              = {https://app.dimensions.ai/details/publication/pub.1175485018},
}

@Article{Narvekar2024,
  author           = {Narvekar, Nisha and Carter Leno, Virginia and Pasco, Greg and Begum Ali, Jannath and Johnson, Mark H. and Charman, Tony and Jones, Emily J. H.},
  journal          = {Journal of Child Psychology and Psychiatry},
  title            = {The roles of sensory hyperreactivity and hyporeactivity in understanding infant fearfulness and emerging autistic traits},
  year             = {2024},
  fundingsection   = {Acknowledgements
