@Article{Modabbernia2022,
  author           = {Modabbernia, Amirhossein and Vidal-Pineiro, Didac and Agartz, Ingrid and Andreassen, Ole A and Ayesa-Arriola, Rosa and Bertolino, Alessandro and Boomsma, Dorret I and Bourque, Josiane and Breier, Alan and Brodaty, Henry and Brouwer, Rachel M and Buitelaar, Jan K and Canales-Rodríguez, Erick J and Caseras, Xavier and Conrod, Patricia J and Crespo-Facorro, Benedicto and Crivello, Fabrice and Crone, Eveline A and de Zubicaray, Greig I and Dickie, Erin W and Dima, Danai and Frenzel, Stefan and Fisher, Simon E and Franke, Barbara and Glahn, David C and Grabe, Hans-Jörgen and Grotegerd, Dominik and Gruber, Oliver and Guerrero-Pedraza, Amalia and Gur, Raquel E and Gur, Ruben C and Hartman, Catharina A and Hoekstra, Pieter J and Pol, Hilleke E Hulshoff and Jahanshad, Neda and Jernigan, Terry L and Jiang, Jiyang and Kalnin, Andrew J and Kochan, Nicole A and Mazoyer, Bernard and McDonald, Brenna C and McMahon, Katie L and Nyberg, Lars and Oosterlaan, Jaap and Pomarol-Clotet, Edith and Radua, Joaquim and Sachdev, Perminder S and Satterthwaite, Theodore D and Salvador, Raymond and Sarro, Salvador and Saykin, Andrew J and Schumann, Gunter and Smoller, Jordan W and Sommer, Iris E and Espeseth, Thomas and Thomopoulos, Sophia I and Trollor, Julian N and van ‘t Ent, Dennis and Voineskos, Aristotle and Wang, Yang and Weber, Bernd and Westlye, Lars T and Whalley, Heather C and Williams, Steven CR and Wittfeld, Katharina and Wright, Margaret J and Thompson, Paul M and Paus, Thomas and Frangou, Sophia},
  journal          = {bioRxiv},
  title            = {Dissociable Cellular and Genetic Mechanisms of Cortical Thinning at Different Life Stages},
  year             = {2022},
  note             = {https://www.biorxiv.org/content/biorxiv/early/2022/03/22/2022.03.21.484899.full.pdf},
  pages            = {2022.03.21.484899},
  abstract         = {Abstract
                Mechanisms underpinning age-related variations in cortical thickness in the human brain remain poorly understood. We investigated whether inter-regional age-related variations in cortical thinning (in a multicohort neuroimaging dataset from the ENIGMA Lifespan Working Group totalling 14,248 individuals, aged 4-89 years) depended on cell-specific marker gene expression levels. We found differences amidst early-life (<20 years), mid-life (20-60 years), and late-life (>60 years) in the patterns of association between inter-regional profiles of cortical thickness and expression profiles of marker genes for CA1 and S1 pyramidal cells, astrocytes, and microglia. Gene ontology and enrichment analyses indicated that each of the three life-stages was associated with different biological processes and cellular components: synaptic modeling in early life, neurotransmission in mid-life, and neurodegeneration in late-life. These findings provide mechanistic insights into age-related cortical thinning during typical development and aging.},
  creationdate     = {2025-05-22T21:36:32},
  doi              = {10.1101/2022.03.21.484899},
  modificationdate = {2025-05-22T21:36:32},
  url              = {https://app.dimensions.ai/details/publication/pub.1146484024},
}

@Article{Janssen2022,
  author           = {Janssen, Joost and Alloza, Clara and Díaz-Caneja, Covadonga M. and Santonja, Javier and Pina-Camacho, Laura and Gordaliza, Pedro M. and Fernández-Pena, Alberto and Lois, Noemi González and Buimer, Elizabeth E.L. and van Haren, Neeltje E.M. and Cahn, Wiepke and Vieta, Eduard and Castro-Fornieles, Josefina and Bernardo, Miquel and Arango, Celso and Kahn, René S. and Pol, Hilleke E. Hulshoff and Schnack, Hugo G.},
