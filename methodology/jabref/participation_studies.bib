@InCollection{DesormeauxMoreau2024,
  author           = {Désormeaux-Moreau, Marjorie and Courcy, Isabelle},
  publisher        = {Springer Nature Switzerland},
  title            = {Neuromixed participatory action research: a dialogical method},
  year             = {2024},
