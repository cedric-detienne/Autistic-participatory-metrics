  groups           = {Bib_check_yes},
  keywords         = {Autism, Allistic self-disempowerment, Dialogical method, Emancipatory approach, Equity, Neurominorized researcher, Neuromixed research collaboration, Neuronormalized researcher, Participatory action research standpoint},
  language         = {en},
  modificationdate = {2025-05-17T22:21:34},
  qualityassured   = {qualityAssured},
  shorttitle       = {Neuromixed {Participatory} {Action} {Research}},
