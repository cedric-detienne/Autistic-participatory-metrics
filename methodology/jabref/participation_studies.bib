  pmid             = {38757637},
  publisher        = {SAGE Publications},
  qualityassured   = {qualityAssured},
  url              = {https://www.semanticscholar.org/paper/00ac7906170855daddd105aa5dd465e52912111f},
  venue            = {Autism},
}

