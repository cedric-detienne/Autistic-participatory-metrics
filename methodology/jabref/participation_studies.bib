  pages = {943--953},
  qualityassured = {qualityAssured},
  shortjournal = {Autism},
  shorttitle = {Making the future together},
  title = {Making the future together: shaping autism research through meaningful participation},
  urldate = {2021-11-22},
  volume = {23},
  year = {2019}
