@article{MacLeod2019,
  abstract = {Purpose
Interpretative phenomenological analysis is distinctive in foregrounding participants’ interpretations. As such, it is potentially useful for gaining an insight into the lived experience of autistic individuals and aligns well with participatory approaches. This paper provides the first systematic review of IPA studies that have employed a participatory approach and considers their contribution to Critical Autism Studies.

Method
Four electronic databases were searched to identify peer-reviewed empirical research articles which employed a participatory IPA approach to consult with autistic individuals. Thirteen studies from four countries, published 2008–2018, met the inclusion criteria. These were examined to identify details of the methodology, findings and recommendations in order to consider each in relation to the underpinning philosophies of IPA and participatory research respectively, and their relationship to the stated aims of Critical Autism Studies.

Results
A range of participatory approaches were employed and their use extended the depth and validity of findings. There was evidence of disruption to power relations in process and outcomes. Making sense of the autism diagnosis was a frequent point of discussion, even when this was not the focus of the study, and across studies, strong commonalities emerged, capturing a multi-dimensionality of ‘autistic identities’.

Conclusion
This review provides evidence of the usefulness of IPA as a research approach to consult with autistic individuals. It lends itself to a range of participatory techniques and there is scope for these to be used with diverse autistic populations, in order to extend the opportunities for autistic individuals to be represented, and represent themselves, within autism research.},
  author = {Andrea MacLeod},
  creationdate = {2025-05-16T08:38:05},
  doi = {10.1016/J.RASD.2019.04.005},
  file = {:MacLeod2019.pdf:PDF},
  issn = {1750-9467},
  journal = {Research in Autism Spectrum Disorders},
  modificationdate = {2025-05-16T09:32:14},
  pages = {49-62},
  publisher = {Elsevier BV},
  qualityassured = {qualityAssured},
  title = {Interpretative phenomenological analysis (ipa) as a tool for participatory research within critical autism studies: a systematic review},
  url = {https://www.semanticscholar.org/paper/d28a5389b3ef03f3827690f51c51b5b6a17900d5},
  venue = {Research in Autism Spectrum Disorders},
  volume = {64},
  year = {2019}
}

@article{Pellicano2019,
  abstract = {There is a growing movement toward participatory research in the field of autism (Chown et al., 2017; Fletcher-Watson et al., 2019; Nicolaidis et al., 2011). This movement is in part driven by a dissatisfaction by the Autistic and autism communities with the preponderance of research focusing on the underlying biology and causes of autism rather than on research that has a meaningful impact on people’s dayto-day lives (Pellicano, Dinsmore, & Charman, 2014). In our view, this reflects a wider movement in healthrelated fields to democratize the research process, ensuring that community members are involved in decisions around research, including setting priorities for research, its design and implementation, and interpretation and dissemination of the findings (e.g., Hickey, 2018; Israel et al., 2005; Jones & Wilsdon, 2018). Involving community members in this way—that is, doing participatory autism research—has the potential to make scientific discoveries more thoroughly relevant to communities, tailored to the realities of their everyday lives and consistent with their values (Chalmers, 2004; Lloyd & White, 2011). Weksler-Derri, Shwed, and Davidovitch (2019) describes one particular variant of this change—research that involved an advisory committee comprised of autistic adults, some who were members of the Autistic Community of Autistic Adults in Israel (ACI) and some who were not, who provided their experiential expertise (cf. Collins & Evans, 2002; Milton, 2014) on the research measures, qualitative themes, and research outcomes. As Weksler-Derri et al. (2019) point out, involving community members in research in this way can raise ethical issues that go beyond more traditional forms of research and which can pose challenges to regulatory governance frameworks, codes of conduct, and ethics review procedures (Banks et al., 2013; Flicker, Travers, Guta, McDonald, & Meagher, 2007; Wilson, Kenny, & Dickson-Swift, 2018). In this instance, the challenges include identifying which community members should be included on advisory boards and how the general commitment to anonymity in the research process should be treated when community participants evince a desire to set such a commitment to one side. In the current article, however, Weksler-Derri et al. (2019) attempt to resolve these ethical disputes without explicit reference to the fundamental ideal underlying this kind of work. This strikes us as a mistake. A compelling foundational ideal can play a key point of reference in ongoing ethical discussion and consideration, making the absence of one from Weksler-Derri et al.’s (2019) discussion problematic. This absence makes it extremely difficult to resolve the practical ethical issues that arise during a participatory research process, as there is little way of ranking potential outcomes against each other. Identifying such an overarching ideal can be difficult, of course, and that difficulty might explain the absence here. There are many potentially compelling commitments that arise in complex matters of research into health and wellbeing, and they can often appear incompatible. Fortunately, the very notion of “democratizing” the research process—a notion that is at least implicit in the current article—could have provided a means by which to identify such an ideal. In the remainder of this brief commentary, we set out how that could have worked. The desire to “democratize” any process involves recognizing that each participant deserves some equality of respect in that process and that means, in turn, that they must be granted relative equality of standing in any decision-making process (Allen, 2016). This, in turn, requires that inequalities of power and influence, often either hidden or ignored in the research process, become the subject of considerable ethical attention. Such inequalities of power and influence matter both because they are likely to have arbitrarily emerged across time and because they are likely to result in one participant or groups of participants having greater or lesser respect and standing than others in the research process. Elucidating the ideal in this way would enable a far clearer approach to the ethical issues raised by Weksler-Derri et al. (2019) to emerge. To give some examples: being alert to the need to ensure all participants are granted equality of respect and standing should lead researchers—and ethical review 858524 JREXXX10.1177/1556264619858524aJournal of Empirical Research on Human Research EthicsPellicano and Stears research-article2019},
  author = {Pellicano, Elizabeth and Stears, Marc},
  creationdate = {2025-05-16T08:38:05},
  doi = {10.1177/1556264619858524a},
  file = {:Pellicano2019.pdf:PDF},
  issn = {1556-2646},
  journal = {Journal of Empirical Research on Human Research Ethics},
  language = {en},
  modificationdate = {2025-05-16T09:32:14},
  month = {December},
  number = {5},
  pages = {452--454},
  publisher = {SAGE Publications Inc},
  qualityassured = {qualityAssured},
  shorttitle = {Commentary 1},
  title = {Commentary 1: {Weksler}-{Derri} et al.’s “{Ethical} {Challenges} in {Participatory} {Research} with {Autistic} {Adults} in {Israel}”},
  urldate = {2024-11-16},
  volume = {14},
  year = {2019}
}

@article{Pellicano2020,
  abstract = {Autism science has transformed beyond recognition in the last two decades. International investment has grown extensively and the number of papers published on autism has increased 10-fold (Pellicano et al., 2014), far surpassing publications on related topics. The sheer amount of scientific research on autism has no doubt been instrumental in many of the discoveries and insights so eloquently described by Happé and Frith (2020). But, as autistic scientist Michelle Dawson reminds us, quality matters too and, for that reason, it is a delight to recognise the contribution that both Happé and Frith have made, dramatically changing our understanding of autism in a host of ways.},
